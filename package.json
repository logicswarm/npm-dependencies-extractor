{
  "name": "npm-dependencies-extractor",
  "version": "0.1.0",
  "description": "Retrieves the (flat) list of package dependencies for Javascript projects using npm",
  "main": "index.js",
  "repository": "philips-software/npm-dependencies-extractor",
  "scripts": {
    "lint": "eslint src/**",
    "test": "jest src",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage src",
    "extract-dependencies": "node ./src/index.js"
  },
  "author": "Sanda Contiu <sanda.contiu@philips.com>",
  "license": "ISC",
  "bin": {
    "extract-dependencies": "./src/index.js"
  },
  "keywords": [
    "dependencies",
    "npm",
    "sbom",
    "software bill of material",
    "flat list",
    "extract",
    "retrieve",
    "dependencies flat list",
    "extract dependencies",
    "list dependencies"
  ],
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.3.0"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^5.0.0",
    "fs-extra": "^9.0.0"
  }
}
