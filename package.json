    "npm",
    "sbom",
    "software bill of material",
    "flat list",
    "extract",
    "retrieve",
    "dependencies flat list",
    "extract dependencies",
    "list dependencies"
  ],
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "jest": "^24.1.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1"
  }
}
